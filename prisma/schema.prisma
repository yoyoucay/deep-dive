// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model News {
  id      Int    @id @default(autoincrement())
  slug    String @unique
  title   String
  content String
  date    String
  image   String

  @@map("news")
}
